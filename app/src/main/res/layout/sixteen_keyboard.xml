<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/shape_f1f4fd_3dp">

    <TextView
        android:id="@+id/tv_num_7"
        android:layout_width="@dimen/dp_42"
        android:layout_height="@dimen/dp_29"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_10"
        android:background="@mipmap/bg_short"
        android:gravity="center"
        android:tag="7"
        android:text="7"
        android:textColor="@color/color_2F2F2F"
        android:textSize="@dimen/sp_13" />

    <TextView
        android:id="@+id/tv_num_8"
        android:layout_width="@dimen/dp_42"
        android:layout_height="@dimen/dp_29"
        android:layout_marginLeft="@dimen/dp_4_5"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_toRightOf="@+id/tv_num_7"
        android:background="@mipmap/bg_short"
        android:gravity="center"
        android:tag="8"
        android:text="8"
        android:textColor="@color/color_2F2F2F"
        android:textSize="@dimen/sp_13" />

    <TextView
        android:id="@+id/tv_num_9"
        android:layout_width="@dimen/dp_42"
        android:layout_height="@dimen/dp_29"
        android:layout_marginLeft="@dimen/dp_4_5"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_toRightOf="@id/tv_num_8"
        android:background="@mipmap/bg_short"
        android:gravity="center"
        android:tag="9"
        android:text="9"
        android:textColor="@color/color_2F2F2F"
        android:textSize="@dimen/sp_13" />

    <FrameLayout
        android:id="@+id/tv_clear"
        android:layout_width="@dimen/dp_42"
        android:layout_height="@dimen/dp_29"
        android:layout_marginLeft="@dimen/dp_4_5"
        android:layout_marginRight="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_toRightOf="@+id/tv_num_9"
        android:background="@mipmap/bg_short"
        android:gravity="center"
        android:tag="-1">
        <ImageView
            android:layout_width="@dimen/dp_15"
            android:layout_height="@dimen/dp_11"
            android:layout_gravity="center"
            android:scaleType="centerCrop"
            android:src="@mipmap/keydel" />
    </FrameLayout>

    <TextView
        android:id="@+id/tv_num_4"
        android:layout_width="@dimen/dp_42"
        android:layout_height="@dimen/dp_29"
        android:layout_below="@id/tv_num_7"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_4"
        android:background="@mipmap/bg_short"
        android:gravity="center"
        android:tag="4"
        android:text="4"
        android:textColor="@color/color_2F2F2F"
        android:textSize="@dimen/sp_13" />

    <TextView
        android:id="@+id/tv_num_5"
        android:layout_width="@dimen/dp_42"
        android:layout_height="@dimen/dp_29"
        android:layout_below="@id/tv_num_8"
        android:layout_marginLeft="@dimen/dp_4_5"
        android:layout_marginTop="@dimen/dp_4"
        android:layout_toRightOf="@+id/tv_num_4"
        android:background="@mipmap/bg_short"
        android:gravity="center"
        android:tag="5"
        android:text="5"
        android:textColor="@color/color_2F2F2F"
        android:textSize="@dimen/sp_13" />

    <TextView
        android:id="@+id/tv_num_6"
        android:layout_width="@dimen/dp_42"
        android:layout_height="@dimen/dp_29"
        android:layout_below="@id/tv_num_9"
        android:layout_marginLeft="@dimen/dp_4_5"
        android:layout_marginTop="@dimen/dp_4"
        android:layout_toRightOf="@id/tv_num_5"
        android:background="@mipmap/bg_short"
        android:gravity="center"
        android:tag="6"
        android:text="6"
        android:textColor="@color/color_2F2F2F"
        android:textSize="@dimen/sp_13" />

    <TextView
        android:id="@+id/tv_delete"
        android:layout_width="@dimen/dp_42"
        android:layout_height="@dimen/dp_29"
        android:layout_below="@id/tv_clear"
        android:layout_marginLeft="@dimen/dp_4_5"
        android:layout_marginTop="@dimen/dp_4"
        android:layout_toRightOf="@+id/tv_num_6"
        android:background="@mipmap/bg_short"
        android:gravity="center"
        android:tag="-2"
        android:text="清空"
        android:textColor="@color/color_2F2F2F"
        android:textSize="@dimen/sp_13" />

    <TextView
        android:id="@+id/tv_num_1"
        android:layout_width="@dimen/dp_42"
        android:layout_height="@dimen/dp_29"
        android:layout_below="@id/tv_num_4"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_4"
        android:background="@mipmap/bg_short"
        android:gravity="center"
        android:tag="1"
        android:text="1"
        android:textColor="@color/color_2F2F2F"
        android:textSize="@dimen/sp_13" />

    <TextView
        android:id="@+id/tv_num_2"
        android:layout_width="@dimen/dp_42"
        android:layout_height="@dimen/dp_29"
        android:layout_below="@id/tv_num_5"
        android:layout_marginLeft="@dimen/dp_4_5"
        android:layout_marginTop="@dimen/dp_4"
        android:layout_toRightOf="@+id/tv_num_1"
        android:background="@mipmap/bg_short"
        android:gravity="center"
        android:tag="2"
        android:text="2"
        android:textColor="@color/color_2F2F2F"
        android:textSize="@dimen/sp_13" />

    <TextView
        android:id="@+id/tv_num_3"
        android:layout_width="@dimen/dp_42"
        android:layout_height="@dimen/dp_29"
        android:layout_below="@id/tv_num_6"
        android:layout_marginLeft="@dimen/dp_4_5"
        android:layout_marginTop="@dimen/dp_4"
        android:layout_toRightOf="@id/tv_num_2"
        android:background="@mipmap/bg_short"
        android:gravity="center"
        android:tag="3"
        android:text="3"
        android:textColor="@color/color_2F2F2F"
        android:textSize="@dimen/sp_13" />

    <TextView
        android:id="@+id/tv_num_0"
        android:layout_width="@dimen/dp_42"
        android:layout_height="@dimen/dp_29"
        android:layout_below="@id/tv_num_1"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_4"
        android:layout_marginBottom="@dimen/dp_10"
        android:background="@mipmap/bg_short"
        android:gravity="center"
        android:tag="0"
        android:text="0"
        android:textColor="@color/color_2F2F2F"
        android:textSize="@dimen/sp_13" />

    <TextView
        android:id="@+id/tv_num_"
        android:layout_width="@dimen/dp_42"
        android:layout_height="@dimen/dp_29"
        android:layout_below="@id/tv_num_2"
        android:layout_marginLeft="@dimen/dp_4_5"
        android:layout_marginTop="@dimen/dp_4"
        android:layout_toRightOf="@+id/tv_num_0"
        android:background="@mipmap/bg_short"
        android:gravity="center"
        android:tag="."
        android:text="."
        android:textColor="@color/color_2F2F2F"
        android:textSize="@dimen/sp_13" />

    <TextView
        android:id="@+id/tv_num_00"
        android:layout_width="@dimen/dp_42"
        android:layout_height="@dimen/dp_29"
        android:layout_below="@+id/tv_num_3"
        android:layout_marginLeft="@dimen/dp_4_5"
        android:layout_marginTop="@dimen/dp_4"
        android:layout_toRightOf="@id/tv_num_"
        android:background="@mipmap/bg_short"
        android:gravity="center"
        android:tag="00"
        android:text="00"
        android:textColor="@color/color_2F2F2F"
        android:textSize="@dimen/sp_13" />

    <TextView
        android:id="@+id/tv_confirm"
        android:layout_width="@dimen/dp_41"
        android:layout_height="61dp"
        android:layout_below="@id/tv_delete"
        android:layout_marginLeft="@dimen/dp_5"
        android:layout_marginTop="@dimen/dp_4_5"
        android:layout_toRightOf="@id/tv_num_6"
        android:background="@drawable/layerlist_1ec1b3_2dp"
        android:gravity="center"
        android:tag="-3"
        android:text="确定"
        android:textColor="@color/white"
        android:textSize="@dimen/sp_13" />

</RelativeLayout>